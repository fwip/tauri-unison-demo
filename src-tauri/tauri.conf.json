{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tauri-unison-demo",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "all": true,
        "scope": ["*", "$RESOURCE/*"]
      },
      "path": {
        "all": true
      },
      "shell": {
        "sidecar": true,
        "scope": [
          {
            "name": "binaries/ucm",
            "sidecar": true,
            "args": [
              "run.compiled",
              {
                "validator": "resources/\\S+"
              }
            ]
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.fwip.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "externalBin": [
        "binaries/ucm"
      ],
      "resources": [
        "resources/*"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-unison-demo",
        "width": 800,
        "height": 600
      }
    ]
  }
}
